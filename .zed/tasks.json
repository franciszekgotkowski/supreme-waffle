[
	{
		"label": "cmake",
		"cwd": "$ZED_WORKTREE_ROOT/build",
		"command": "cmake -S ../ -G \"MinGW Makefiles\""
	},
	{
		"label": "make",
		"cwd": "$ZED_WORKTREE_ROOT/build",
